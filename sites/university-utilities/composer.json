    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
   "psr-4": {
     "Drupal\\feeds_role_cleanup_option\\": "web/modules/custom/feeds_role_cleanup_option/src/"
   },
   "exclude-from-classmap": [
            "vendor/simplesamlphp/*"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "ewcomposer/unpack": true,
            "simplesamlphp/composer-module-installer": true,
            "simplesamlphp/composer-xmlprovider-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2",
            "drupal/contrib-module1": "-p1",
            "drupal/contrib-module2": "-p1"
        },
    "patches": {
      "drupal/core": {
        "Add 'Select All' to table headers for accessibility": "https://www.drupal.org/files/issues/2024-01-18/2766853-54.patch"
      },
      "drupal/flysystem": {
        "Image style generation for Flysystem remote schemes": "https://www.drupal.org/files/issues/2025-03-18/3513799-3-image-style-generation.patch"
      }
    },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/recipes/contrib/{$name}": ["type:drupal-recipe"]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>ECA and Drupal Recipes</>",
                "<bg=blue;fg=white>Pure Butter!</>"
            ]
        },
        "installer-types": ["drupal-recipe"]
    }
}
